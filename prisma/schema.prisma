datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id String @default(cuid()) @id
  email String @unique
  userId String @unique
  password String?
  createdAt DateTime @default(now())
}

model Post {
  postId String @default(cuid()) @id
  id String
  title String
  contents String?
  createdAt DateTime @default(now())
}