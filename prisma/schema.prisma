datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  email String @id
  id String
  userId String
  password String
  createdAt DateTime @default(now())
}

model Post {
  postId String @id
  id String
  contents String
  createdAt DateTime @default(now())
}